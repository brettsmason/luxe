{
  "name": "luxe",
  "version": "1.0.0",
  "description": "A starter theme for WordPress.",
  "main": "index.js",
  "scripts": {
    "start": "node ./resources/build/cli-start.js",
    "dev": "webpack --env.development --config ./resources/build/webpack.config.js",
    "build": "webpack -p --env.production --config ./resources/build/webpack.config.js",
    "watch": "npm run dev -- --watch && webpack-dev-server",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:styles": "stylelint './resources/styles/**/*.scss' --syntax scss",
    "lint:scripts": "eslint './resources/scripts/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettsmason/luxe.git"
  },
  "author": "Brett Mason",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/brettsmason/luxe/issues"
  },
  "homepage": "https://github.com/brettsmason/luxe#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "babel-loader": "^8.0.0-beta.2",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "browserslist": "^3.2.5",
    "chalk": "^2.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "clear": "^0.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-wordpress": "^2.0.0",
    "figlet": "^1.2.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "img-loader": "^2.0.1",
    "inquirer": "^5.2.0",
    "lost": "^8.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-scss": "^1.0.5",
    "replace-in-file": "^3.4.0",
    "sass-loader": "^7.0.1",
    "sass-mq": "^4.0.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "stylelint": "^9.2.1",
    "stylelint-config-wordpress": "^13.0.0",
    "typi": "zellwk/typi",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^2.4.4",
    "webpack-extraneous-file-cleanup-plugin": "^2.0.0",
    "webpack-watched-glob-entries-plugin": "^2.1.2"
  }
}
